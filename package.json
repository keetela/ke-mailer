{
  "name": "keemailer",
  "version": "1.0.0",
  "description": "Keemailer API microservice",
  "main": "src/index.js",
  "private": true,
  "author": "Gilles Kagarama",
  "scripts": {
    "start": "export NODE_ENV=production && npm run db:migrate && babel-node src/index.js",
    "dev": "concurrently  \"nodemon src/index.js --exec babel-node --presets @babel/preset-env\"",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "license": "None",
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@hapi/joi": "^15.1.1",
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.7.3",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.17",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4"
  }
}
